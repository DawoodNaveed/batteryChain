{% extends '@SonataAdmin/CRUD/base_show_field.html.twig' %}

{% block field %}
    {% if value is not null and value != '' %}
        <img style="object-fit: contain" src="{{ value | preSignedUrl(co2_aws_path) }}" height="100" width="100"  alt="logo" class="manufacturer_logo"/>
    {% endif %}
    {% if value is null or value == '' %}
        <img src="{{ asset(no_image_path) }}" height="100" width="100" style="object-fit: contain">
    {% endif %}
{% endblock %}